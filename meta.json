{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "Project name"
    },
    "description": {
      "type": "string",
      "required": true,
      "message": "Project description",
      "default": "A Vue.js 2.0 project"
    },
    "author": {
      "type": "string",
      "message": "Author"
    },
    "jsoutput": {
      "type": "list",
      "message": "Format for bundled JS?",
      "choices": [
        "amd", "cjs", "es", "iife", "umd"
      ]
    }
  }
}
