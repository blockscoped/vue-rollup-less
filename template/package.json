{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "author": "{{ author }}",
  {{#private}}
  "private": true,
  {{/private}}
  "scripts": {
    "watch": "rollup -cw",
    "serve": "http-server -s -c 1 -a localhost -p 10001",
    "browse": "http-server -s -c 1 -a localhost -p 10001 -o",
    "dev": "run-p watch serve",
    "open": "run-p watch browse",
    "build": "cross-env NODE_ENV=production rollup -c"
  },
  "dependencies": {
    "vue": "^2.0.0-rc.7"
  },
  "devDependencies": {
    "cross-env": "^2.0.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.0.0",
    "rollup": "^0.36.0",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-css-only": "0.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-vue2": "0.0.4",
    "rollup-watch": "^2.5.0"
  }
}
